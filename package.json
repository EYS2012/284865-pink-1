{
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "dependencies": {},
  "description": "Личный проект «Пинк» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "7.2.x",
    "browser-sync": "2.23.x",
    "gulp": "3.9.x",
    "gulp-csso": "3.0.1",
    "gulp-less": "3.4.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-rename": "1.2.2",
    "run-sequence": "2.2.1",
    "stylelint": "8.4.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "editorconfig-cli": [
    "source/*.json",
    "source/*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "8.9"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "editorconfig": "editorconfig-cli",
    "start": "npm run build && gulp serve",
    "stylelint": "stylelint 'source/less/**/*.less' --syntax less",
    "test": "npm run editorconfig && npm run stylelint"
  },
  "version": "13.0.0"
